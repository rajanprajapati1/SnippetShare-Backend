{
    "version" : 2 ,
    "builds" : [{
        "src" : "./src/index.js",
        "use" : "@vercel/node"
    }],
     "routes": [
    {
      "src": "/api/(.*)", // Match all API routes
      "dest": "/src/index.js" // Forward to your Express server
    },
    {
      "src": "/(.*)", // Match all other routes
      "dest": "/src/index.js" // Forward to your Express server (if needed)
    }
  ]
}
